<h3>通知</h3>
<%= link_to "通知削除", notification_path(@notifications), method: :delete  %>
<% if @notifications.present? %>
   <% @notifications.each do |notification| %>
     <%= render partial: 'notification', locals: {notification: notification} %>
<% end %>
<% else %>
    <p>通知はありません</p>
<% end %>

