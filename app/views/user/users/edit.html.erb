<%= form_with model: @user,url: user_path, method: :patch do |f| %>


  <table class="jj">
    <thead>
      <th></th>
      <th></th>
      <th></th>
    </thead>
    <tbody>
      <tr>
        <td><%=f.label :image,"ユーザー画像"%></td>
        <td colspan="2"><%= f.file_field :image, accept: "image/*" %></td>
      </tr>
      <tr>
        <td><%=f.label :nickname,"ニックネーム"%></td>
        <td colspan="2"><%=f.text_field :nickname%></td>
      </tr>
      <tr>
        <td><%=f.label :age,"年齢"%></td>
        <td colspan="2"><%= f.select :age, [['10代','10代'], ['20代', '20代'], ['30代', '30代'], ['40代','40代'], ['50代','50代'], ['60代','60代'], ['70代','70代'],['非公開','非公開']]%></td>  <!--ageをinteger型じゃなくてstring型にして['20代', "20代"]でもいい-->
      <tr>
        <td><%=f.label :gender,"性別"%></td>
        <td><%= f.radio_button :gender, "男性" %>男</td>
        <td><%= f.radio_button :gender, "女性" %>女</td>
        <td><%= f.radio_button :gender, "非公開" %>非公開</td>
      </tr>
      <tr>
        <td></td>
        <td colspan="2"><%= f.submit "変更する",class: "btn"%></td>
      </tr>
     </tbody>


<%end%>